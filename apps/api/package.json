{
  "name": "api",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup --watch --onSuccess \"node dist/index.cjs\"",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/ --max-warnings 0",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "@tripie-pyotato/jest/server"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.58",
    "ai": "^3.3.30",
    "body-parser": "^1.20.3",
    "cloudinary": "^2.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "morgan": "^1.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@tripie-pyotato/eslint-config": "workspace:*",
    "@tripie-pyotato/jest": "workspace:*",
    "@tripie-pyotato/typescript-config": "^0.0.1",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.9",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "tsup": "^8.4.0",
    "typescript": "5.8.2"
  }
}
