{
  "name": "@tripie-pyotato/design-system",
  "version": "0.1.8",
  "description": "트리피에 사용되는 디자인 시스템",
  "main": "index.js",
  "type": "module",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./shared": {
      "require": {
        "types": "./dist/shared/index.d.cts",
        "default": "./dist/shared/index.cjs"
      },
      "import": {
        "types": "./dist/shared/index.d.ts",
        "default": "./dist/shared/index.js"
      }
    },
    "./@hooks": {
      "require": {
        "types": "./dist/hooks/index.d.cts",
        "default": "./dist/hooks/index.cjs"
      },
      "import": {
        "types": "./dist/hooks/index.d.ts",
        "default": "./dist/hooks/index.js"
      }
    },
    "./@components/core": {
      "require": {
        "types": "./dist/components/core/index.d.cts",
        "default": "./dist/components/core/index.cjs"
      },
      "import": {
        "types": "./dist/components/core/index.d.ts",
        "default": "./dist/components/core/index.js"
      }
    },
    "./@components": {
      "require": {
        "types": "./dist/components/index.d.cts",
        "default": "./dist/components/index.cjs"
      },
      "import": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/components/index.js"
      }
    },
    "./@wrappers": {
      "require": {
        "types": "./dist/wrappers/index.d.cts",
        "default": "./dist/wrappers/index.cjs"
      },
      "import": {
        "types": "./dist/wrappers/index.d.ts",
        "default": "./dist/wrappers/index.js"
      }
    },
    "./global": {
      "import": "./dist/global.css",
      "require": "./dist/global.css",
      "default": "./dist/global.css"
    },
    "./variables": {
      "import": "./dist/variables.css",
      "require": "./dist/variables.css",
      "default": "./dist/variables.css"
    },
    "./styles": {
      "import": "./dist/index.css",
      "require": "./dist/index.css",
      "default": "./dist/index.css"
    }
  },
  "scripts": {
    "build": "tsup --inject ./react-import.ts",
    "metafile": "tsup --format cjs,esm",
    "build:pkg": "tsc",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "pnpm run lint --fix",
    "generate:component": "turbo gen react-component",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "clean": "rm -rf ./node_modules && rm -rf ./.turbo && rm -rf ./dist && rm -rf ./.next"
  },
  "devDependencies": {
    "@storybook/react": "^8.2.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@tripie-pyotato/eslint-config": "^0.0.1",
    "@tripie-pyotato/typescript-config": "^0.0.2",
    "@turbo/gen": "^2.0.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^29.7.0",
    "bundle-scss": "^1.5.4",
    "css-loader": "^7.1.2",
    "csstype": "^3.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lit-element": "^4.1.1",
    "next": "^15.0.3",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-modules": "^6.0.1",
    "postcss-utilities": "^0.8.4",
    "react": "^18.3.1",
    "sass": "^1.85.0",
    "ts-jest": "^29.2.4",
    "tsup": "^8.4.0",
    "vitest": "^3.0.6"
  },
  "dependencies": {
    "@tripie-pyotato/hooks": "^0.0.3",
    "@tripie-pyotato/utils": "^0.0.3",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "esbuild": "^0.25.2",
    "esbuild-sass-plugin": "^3.3.1",
    "framer": "^2.4.1",
    "framer-motion": "^11.11.17",
    "postcss-copy-assets": "^0.3.1",
    "react-calendar": "^5.0.0",
    "react-dom": "^18.3.1",
    "react-globe.gl": "^2.27.2",
    "react-intersection-observer": "9.13.1",
    "rollup-preserve-directives": "^1.1.3",
    "usehooks-ts": "^3.1.0"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "peerDependencies": {
    "next": "*",
    "react": "*"
  }
}
