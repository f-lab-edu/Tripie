{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "!**/*.stories.{tsx,jsx,mdx}"],
      "outputs": [".next/**", "!.next/cache/**", "storybook-static/**", "vercel/**", ".vercel/**", "dist/**"],
      "env": [
        "ALLOWED_METHODS",
        "ALLOWED_HEADERS",
        "MAX_AGE",
        "CREDENTIALS",
        "NEXT_PUBLIC_GOOGLE_MAP_API",
        "NEXT_PUBLIC_GOOGLE_PLACES_API",
        "NEXT_PUBLIC_GPT_API_KEY",
        "OPENAI_API_KEY",
        "NEXT_PUBLIC_CONTINENTL",
        "NEXT_PUBLIC_USER_TOKEN_DB_NAME",
        "CHAT_CACHE_DB_NAME",
        "NEXT_PUBLIC_AWS_PLACE_ACTIONS",
        "NEXT_PUBLIC_AWS_MAP_ACTIONS",
        "NEXT_PUBLIC_AWS_ACCESS_KEY",
        "NEXT_PUBLIC_AWS_SECRET_KEY",
        "NEXT_PUBLIC_GITHUB_ID",
        "NEXT_PUBLIC_GITHUB_SECRET",
        "NEXTAUTH_URL",
        "KAKAO_CLIENT_ID",
        "AUTH_KAKAO_ID",
        "AUTH_KAKAO_SECRET",
        "KAKAO_CLIENT_SECRET",
        "NEXT_GITHUB_ID",
        "NEXT_GITHUB_SECRET",
        "NEXT_PUBLIC_GITHUB_ID",
        "NEXT_PUBLIC_GITHUB_SECRET",
        "NEXTAUTH_SECRET",
        "NX_DAEMON",
        "POSTGRES_DATABASE",
        "POSTGRES_HOST",
        "POSTGRES_PASSWORD",
        "POSTGRES_PRISMA_URL",
        "connect_timeout",
        "POSTGRES_URL",
        "POSTGRES_URL_NON_POOLING",
        "POSTGRES_USER",
        "POSTGRES_URL_NO_SSL",
        "TURBO_REMOTE_ONLY",
        "TURBO_RUN_SUMMARY",
        "VERCEL",
        "VERCEL_ENV",
        "VERCEL_URL",
        "PRISMA_QUERY_ENGINE_LIBRARY",
        "NEXT_PUBLIC_DOMAIN"
      ]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "env": [
        "OPENAI_API_KEY",
        "NEXTAUTH_SECRET",
        "POSTGRES_URL",
        "NEXT_PUBLIC_USER_TOKEN_DB_NAME",
        "POSTGRES_URL_NON_POOLING",
        "POSTGRES_URL_NO_SSL",
        "POSTGRES_PRISMA_URL",
        "POSTGRES_USER",
        "POSTGRES_PASSWORD",
        "POSTGRES_HOST",
        "POSTGRES_DATABASE",
        "KAKAO_CLIENT_ID",
        "KAKAO_CLIENT_SECRET",
        "NEXT_PUBLIC_GPT_API_KEY",
        "CHAT_CACHE_DB_NAME",
        "NEXT_PUBLIC_DOMAIN",
        "NEXT_GITHUB_ID",
        "NEXT_GITHUB_SECRET",
        "AUTH_KAKAO_ID",
        "AUTH_KAKAO_SECRET"
      ],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build:storybook": {
      "env": ["CHROMATIC_TOKEN"]
    },
    "build:api": {
      "env": [
        "CLOUDINARY_API_KEY",
        "CLOUDINARY_CLOUD_NAME",
        "CLOUDINARY_API_KEY",
        "CLOUDINARY_API_SECRET",
        "CLOUDINARY_URL",
        "OPENAI_API_KEY",
        "ALLOWED_ORIGIN"
      ]
    },

    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "vercel/**", ".vercel/**", "dist/**"],
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:deploy": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "dependsOn": ["^build"],
      "persistent": true
    },
    "typecheck": {},
    "test": {},
    "test:watch": {
      "cache": false,
      "persistent": true
    }
  },
  "globalDependencies": [".env"]
}
